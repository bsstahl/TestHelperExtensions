<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RootNamespace>TestHelperExtensions</RootNamespace>
    <Version>2.0.0.0</Version>
    <PackageId>TestHelperExtensions</PackageId>
    <Authors>Barry S. Stahl</Authors>
    <Company>CopperByte, Inc.</Company>
    <Product>TestHelperExtensions</Product>
    <Description>A set of extension methods helpful when building unit tests. Methods include functionality for creating random data of various types, converting data types, comparing date data, searching within string arrays, etc.</Description>
    <PackageProjectUrl>http://github.com/bsstahl/TestHelperExtensions</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/bsstahl/TestHelperExtensions/blob/master/LICENSE</PackageLicenseUrl>
    <PackageTags>testing unit test extensions helper helpers</PackageTags>
    <PackageReleaseNotes>Version 2.0.0.0 - Now targets the .NET Standard 2.0 interface</PackageReleaseNotes>
    <NeutralLanguage>EN-US</NeutralLanguage>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

</Project>
